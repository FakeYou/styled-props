{
  "name": "styled-props",
  "version": "v1.0.0",
  "description": "Small helper to style your components with style.",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src __test__",
    "compile": "babel -d lib src",
    "compile:watch": "babel -w -d lib src",
    "clean": "rm -rf lib",
    "prepublish": "npm run clean && npm test && npm run compile",
    "precommit": "lint-staged",
    "format": "prettier --single-quote --trailing-comma=es5 --parser=flow --write \"src/**/*.js\""
  },
  "files": [
    "*.md",
    "__test__",
    "LICENSE",
    "lib",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RafalFilipek/styled-props.git"
  },
  "bugs": {
    "url": "https://github.com/RafalFilipek/styled-props/issues"
  },
  "author": "Rafa≈Ç Filipek <rafal.filipek@gmail.com>",
  "license": "MIT",
  "keywords": [
    "css",
    "css-in-js"
  ],
  "lint-staged": {
    "*.js": [
      "prettier --single-quote --trailing-comma=es5 --parser=babylon --write",
      "git add"
    ]
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./.coverage",
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.0",
    "eslint-plugin-flowtype-errors": "^3.3.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.0",
    "eslint-plugin-react": "^7.0.1",
    "flow-bin": "^0.47.0",
    "husky": "^0.13.4",
    "jest": "^20.0.1",
    "lint-staged": "^3.6.0",
    "prettier": "^1.4.2",
    "publish-please": "^2.3.1",
    "react": "^15.5.4",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.5.4",
    "release": "^1.2.3",
    "styled-components": "^2.0.0"
  },
  "dependencies": {}
}